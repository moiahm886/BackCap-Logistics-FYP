@model Organization

<div>
    <h2>Delete Organization</h2>
    <form method="post" action="@Url.Action("DeleteOrganization", "Vehicle")">
        <input type="hidden" asp-for="OrganizationId" />
        <button type="submit" onclick="return confirm('Are you sure you want to delete this organization?')">Delete</button>
    </form>
</div>
